#. [objective]: condition=lose
#: The_Memories/utils/deaths.cfg:9
msgid "Death of Tendriel"
msgstr ""

#. [objective]: condition=lose
#: The_Memories/utils/deaths.cfg:18
msgid "Death of Gioniel"
msgstr ""

#. [objective]: condition=lose
#: The_Memories/utils/deaths.cfg:28
msgid "Death of Tináclya"
msgstr ""

#. [message]: speaker=unit
#: The_Memories/utils/deaths.cfg:48
msgid "I will never know who am I."
msgstr ""

#. [message]: speaker=second_unit
#: The_Memories/utils/deaths.cfg:52
msgid "It may be better."
msgstr ""

#. [message]: speaker=Elrilinde
#: The_Memories/utils/deaths.cfg:80
msgid "No! I will remember nothing without him."
msgstr ""

#. [message]: speaker=Elrilinde
#: The_Memories/utils/deaths.cfg:108
msgid "No! I will remember nothing without her."
msgstr ""

#. [message]: speaker=Elrilinde
#: The_Memories/utils/deaths.cfg:142
msgid "Farewell, $unit.name. I will miss you."
msgstr ""

